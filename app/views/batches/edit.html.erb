<h1>Editing batch</h1>

<% form_for(@batch) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :applicant_batch %><br />
    <%= f.text_field :applicant_batch %>
  </p>
  <p>
    <%= f.label :year %><br />
    <%= f.text_field :year %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @batch %> |
<%= link_to 'Back', batches_path %>
